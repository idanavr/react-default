{
  "name": "react-default",
  "version": "1.0.0",
  "description": "Starter kit for reactjs",
  "main": "index.js",
  "scripts": {
    "start": "SET NODE_ENV=production && node server/server.js",
    "develop": "npm-run-all --parallel serverdebug:open *:watch",
    "serverdebug:open": "SET NODE_ENV=development && nodemon server/server.js --watch ./server",
    "prebuild": "npm-run-all --parallel test lint",
    "build": "rimraf public && webpack --config ./server/settings/webpack.config.prod",
    "preadd": "npm run build",
    "add": "git add .",
    "test-client": "mocha --require ignore-styles --compilers js:babel-core/register client/**/*.test.js",
    "test": "mocha --exit server/**/*.spec.js",
    "test:watch": "mocha server/**/*.spec.js -- --watch",
    "lint": "esw client server",
    "lint:watch": "npm run lint -- --watch"
  },
  "author": "Idan Avrahami",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mongodb": "^3.5.1",
    "mongoose": "^5.8.9",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-ga": "^2.7.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "eslint-watch": "^6.0.1",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^16.0.1",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.0.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-md5-hash": "^0.0.6",
    "yarn": "^1.22.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "keywords": [
    "react default",
    "react",
    "redux",
    "nodejs",
    "chai",
    "mongoose",
    "jwt",
    "eslint"
  ]
}
